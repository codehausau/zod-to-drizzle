{
  "name": "@codehaus/zod-to-drizzle",
  "version": "1.1.0",
  "description": "Create Drizzle ORM tables from Zod schemas",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "build": "tsup",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "drizzle",
    "zod",
    "schema",
    "orm",
    "typescript",
    "sqlite",
    "postgres",
    "mysql"
  ],
  "author": "Codehaus - forked from bil0ak <contact@bilalakkil.com> (https://github.com/bil0ak)",
  "repository": "https://github.com/codehausau/zod-to-drizzle",
  "homepage": "https://github.com/codehausau/zod-to-drizzle",
  "license": "MIT",
  "dependencies": {
    "drizzle-orm": "0.44.3",
    "semantic-release": "24.2.7",
    "zod": "4.0.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "24.0.15",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "typescript": "5.8.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": "22.11.0"
  }
}
